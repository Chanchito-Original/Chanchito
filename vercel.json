{
    "version": 2,
    "builds": [
      { "src": "src/client/package.json", "use": "@vercel/node" },
      { "src": "src/server/package.json", "use": "@vercel/node" }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "src/server/$1.js" },
      { "src": "/(.*)", "dest": "src/client/$1" }
    ]
  }
  